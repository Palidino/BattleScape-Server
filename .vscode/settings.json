{
    "files.encoding": "utf8",
    "files.insertFinalNewline": true,
    "files.exclude": {
        "**/.idea": true,
        "**/.vscode": true,
        "**/.apt_*": true,
        "**/.git": true,
        "**/.github": true,
        "**/.gitmodules": true,
        "**/.gitignore": true,
        "**/.mvn": true,
        "**/mvnw": true,
        "**/mvnw.cmd": true,
        "**/.settings": true,
        "**/.classpath": true,
        "**/.factorypath": true,
        "**/.project": true,
        "**/.DS_Store": true,
        "**/thumbs.db": true,
        "**/Desktop.ini": true,
        "**/desktop.ini": true,
        "**/*.class": true,
        "**/*.jar": true,
        "**/*.zip": true,
        "**/*.gz": true,
        "**/*.log": true,
        "**/log.txt": true,
        "**/nohup*.out": true,
        "**/target": true,
        "**/repo": true
    },
    "search.useIgnoreFiles": false,
    "search.exclude": {
        "**/map/xtea.json": true,
        "**/map/no_xtea.json": true
    },
    "editor.wrappingIndent": "deepIndent",
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 100,
    "java.configuration.updateBuildConfiguration": "interactive",
    "java.format.onType.enabled": true,
    "[java]": {
        "editor.formatOnSave": true
    },
    "javascript.validate.enable": false
}
