{
    "configurations": [
        {
            "type": "java",
            "console": "internalConsole",
            "projectName": "osrs-server",
            "name": "OSRS Eco Server",
            "request": "launch",
            "vmArgs": "-Xmx768m -XX:MinHeapFreeRatio=5 -XX:MaxHeapFreeRatio=25 -XX:MaxGCPauseMillis=50",
            "mainClass": "com.palidinodh.osrsserver.Main",
            "args": "osrs-core/src/main/settings/economy_local.json"
        },
        {
            "type": "java",
            "console": "internalConsole",
            "projectName": "osrs-server",
            "name": "Test World Jar",
            "request": "launch",
            "vmArgs": "-Xmx768m -XX:MinHeapFreeRatio=5 -XX:MaxHeapFreeRatio=25 -XX:MaxGCPauseMillis=50",
            "classPaths": ["osrs-server/target/deploy/world.jar"],
            "mainClass": "com.palidinodh.osrsserver.Main",
            "args": "osrs-core/src/main/settings/economy_local.json"
        }
    ]
}
